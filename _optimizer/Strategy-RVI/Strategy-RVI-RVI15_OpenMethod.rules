## Init settings.
# Parameter list to optimize.
param_list=( RVI15_OpenMethod )
# Disable all optimization settings.
input_set .*,F 0
# Disable all active strategies.
input_set .*_Active 0
# Disable account conditions.
#input_set Account_Conditions_Active 0
# Disable boosting.
input_set Boosting_Enabled 0
# Set lot size.
input_set LotSize 0.01
# Enable strategy.
input_set ^RVI15_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test RVI15_OpenMethod.
input_set ^RVI15_OpenMethod,F 1
input_set ^RVI15_OpenMethod,1 0
input_set ^RVI15_OpenMethod,2 1
input_set ^RVI15_OpenMethod,3 127

