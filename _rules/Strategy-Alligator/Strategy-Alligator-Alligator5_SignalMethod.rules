## Init settings.
# Parameter list to optimize.
strategy=Alligator5
param=${strategy}_SignalMethod
param_list=( ${param} )
## Backtest settings.
ini_set ^TestPeriod M5
## EA settings.
ea_strategy_default_sets ${strategy}

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
ea_set_optimize_param ${param} 0 63 3

