## Init settings.
# Parameter list to optimize.
name=Bands
period=5
strategy=${name}${period}
param=${strategy}_SignalMethod
param_list=( ${param} )
## Backtest settings.
ini_set ^TestPeriod M${period}
## EA settings.
ea_strategy_default_sets ${strategy}
ea_set_neutral_trail ${name}

## Optimization rules.
ea_set_optimize_param ${param} 0 255 7

# $ETA: 1h$
